{
  "name": "front",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxi dev",
    "preview": "nuxi preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint './**/*.{js,ts,vue}' --fix",
    "stylelint": "stylelint './**/*.{scss,css,vue}' --fix",
    "lint-staged": "lint-staged",
    "prepare": "cd .. && husky front/.husky"
  },
  "lint-staged": {
    "*.scss": [
      "stylelint --fix"
    ],
    "*.{css,vue}": [
      "stylelint --fix"
    ],
    "*.{js,ts,vue}": [
      "eslint --ext '.js,.ts,.vue' --fix --max-warnings 0",
      "prettier --write"
    ],
    "*.json": [
      "prettier --write"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@fxts/core": "^0.23.0",
    "@pinia/nuxt": "^0.5.1",
    "@vueuse/nuxt": "^10.7.2",
    "axios": "^1.6.7",
    "dummy-package": "^1.0.0",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.1.7",
    "primeicons": "^6.0.1",
    "primevue": "^3.48.1",
    "sass": "^1.70.0",
    "stylelint": "^16.2.1",
    "vue": "^3.4.15",
    "vue-router": "^4.2.5"
  },
  "devDependencies": {
    "@nuxtjs/i18n": "^8.1.0",
    "@vue/eslint-config-prettier": "^9.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "autoprefixer": "^10.4.17",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-simple-import-sort": "^12.0.0",
    "eslint-plugin-vue": "^9.21.1",
    "husky": "^9.0.10",
    "lint-staged": "^15.2.2",
    "nuxt": "^3.10.1",
    "nuxt-primevue": "^0.2.2",
    "nuxt-svgo": "^4.0.0",
    "postcss": "^8.4.35",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "tailwindcss": "^3.4.1"
  }
}
